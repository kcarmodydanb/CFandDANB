CREATE OR REPLACE FORCE VIEW "VW_DANB_EXM_EXAM" ("TX_SERNO", "CUST_ID", "CATEGORY_CD", "LEVEL1", "LEVEL2", "LEVEL3", "LEVEL4", "LEVEL5", "START_DT", "END_DT", "STATUS_CD", "CREDIT_PTS_REQUESTED", "CREDIT_PTS_ALLOWED", "CREDIT_PTS_APPROVED", "CREDIT_UNIT", "CREATE_DT", "CREATE_BY", "LAST_CHANGE_DT", "LAST_CHANGE_BY", "ACTIVE_FL", "RESULT_CD", "RESULT_SCORE", "TARGET_SCORE", "EXAM_VUE_REGID", "EXAM_ID", "EXAM_SCORED_DATE", "EXAM_EDU_EXP_MONTHS", "EXAM_SERIES_CD", "EXAM_GROUP_NO", "EXAM_TEST_DATE", "EXAM_AFDS", "EXAM_EDU_EXP_YEARS") AS 
  SELECT 
tx.TX_SERNO AS TX_SERNO,
CUST_ID,
CATEGORY_CD,
LEVEL1,
LEVEL2,
LEVEL3,
LEVEL4,
LEVEL5,
START_DT,
END_DT,
STATUS_CD,
CREDIT_PTS_REQUESTED,
CREDIT_PTS_ALLOWED,
CREDIT_PTS_APPROVED,
CREDIT_UNIT,
CREATE_DT,
CREATE_BY,
LAST_CHANGE_DT,
LAST_CHANGE_BY,
ACTIVE_FL,
RESULT_CD,
RESULT_SCORE,
TARGET_SCORE,
EXAM_VUE_REGID,
EXAM_ID,
EXAM_SCORED_DATE,
EXAM_EDU_EXP_MONTHS,
EXAM_SERIES_CD,
EXAM_GROUP_NO,
EXAM_TEST_DATE,
EXAM_AFDS,
EXAM_EDU_EXP_YEARS

 FROM CEN_CUST_TX tx LEFT JOIN VW_DANB_EXAM_ATTRIBUTES atr ON tx.TX_SERNO = atr.TX_SERNO WHERE CATEGORY_CD = 'EXAM'
/
